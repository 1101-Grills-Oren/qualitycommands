/**
 * The lightmap texture manager maintains a texture containing the RGBA overlay for each of the 16&times;16 sky and block light combinations.
 * <p>
 * Also contains some utilities to pack and unpack lightmap coordinates from sky and block light values,
 * and some lightmap coordinates constants.
 */
@Mixin(LightmapTextureManager.class)
public class LightmapTextureManagerMixin implements AutoCloseable {
        
//xord=4
        @ModifyVariable(method = "update", at = @At("STORE"), ordinal = 4)
        private int modifyRed(int red){}
        @ModifyVariable(method = "update", at = @At("STORE"), ordinal = 5)
        private int modifyGreen(int green){}
        @ModifyVariable(method = "update", at = @At("STORE"), ordinal = 6)
        private int modifyBlue(int blue){}
        
}